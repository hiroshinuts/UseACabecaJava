package cap1;

public class QuebraCabecaPiscina {
	
	public static void main(String[] args) {
		
		int x = 0;
		
		// SAIDA
		// a noise
		// annoys
		// a oyster
		
		while(x < 4) {
			
			if(x < 3){
				x = x+1;
				System.out.print("a ");
				System.out.print("noise");
			}
			
			if(x > 3){
				
			}
				
			if(x == 1){
				System.out.println("annoys");
				x= x+2;
			}
			
			if(x > 2){
				System.out.print("an ");
				System.out.print("oise");
			}
			System.out.println("");
		}
		
		
	}
	

}
