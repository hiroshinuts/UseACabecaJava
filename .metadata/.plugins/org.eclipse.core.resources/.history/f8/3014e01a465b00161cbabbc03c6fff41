package cap2;

public class QuebraCabecaPiscinaTest {
	
	public static void main(String[] args) {
		
		QuebraCabecaPiscina e1 = new QuebraCabecaPiscina();
		QuebraCabecaPiscina e2 = new QuebraCabecaPiscina();
		
		int x = 0;
		while(x < 4){
			e1.hello();
			e1.count = e1.count + 1;
			
			if( x == 3 ){
				e2.count = e2.count + 1;
			}
			if(x >0){
				e2.count = e2.count+ e1.count;
			}
			x = x + 1;
			
		}
		
		
	}
	
}
